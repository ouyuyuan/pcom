'reinit'

'!stnmap -i sfcuvts.ctl'

'open sfcuvts.ctl'
'set mpdset lowres'
'set poli on'
'set vpage 0.6 10.4 0 8.5'
'set lon 0 360'
'set lat -90 90'
'set xlopts 1 4 0.15'
'set ylopts 1 4 0.15'
'set grid off'
'set grads off'
'set cthick 6'
'set clopts -1 -1 0.15'
'set clab forced'
'display ssh'
'draw title ssh plot'
'printim sfcuvts.png x1600 y1200 white'

'reinit'
'quit'
